{
  "project": {
    "version": "1.0.0-beta.22-1",
    "name": "hapchot-web-radio"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": "assets",
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "styles.scss",
        "../node_modules/dhtmlx-scheduler/codebase/dhtmlxscheduler.css",
        "../vendors/PureJSCarousel/purejscarousel.css"
      ],
      "scripts": [
        "../node_modules/soundmanager2/script/soundmanager2-nodebug-jsmin.js",       
        "../node_modules/dhtmlx-scheduler/codebase/dhtmlxscheduler.js",
        "../node_modules/dhtmlx-scheduler/codebase/locale/locale_fr.js",
        "../vendors/dhtmlx_scheduler_firebase.js",
        "../node_modules/jquery/dist/jquery.js",    
        "../vendors/PureJSCarousel/purejscarousel.js"       
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": false,
      "directive": false,
      "module": false,
      "pipe": false,
      "service": false
    }
  }
}
